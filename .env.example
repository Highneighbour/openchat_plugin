# OpenChat Plugin Configuration Example
# Copy this to .env and fill in your values

# Bot Identity (Required)
# Generate with: openssl ecparam -genkey -name secp256k1 -out private_key.pem
# Then paste the content here with \n for newlines
OPENCHAT_BOT_IDENTITY_PRIVATE_KEY="-----BEGIN EC PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END EC PRIVATE KEY-----"

# OpenChat Configuration (Required)
# Get these from OpenChat profile -> Advanced -> Bot client data
OPENCHAT_PUBLIC_KEY="your-openchat-public-key-here"
OPENCHAT_IC_HOST="https://ic0.app"
OPENCHAT_STORAGE_INDEX_CANISTER="your-storage-canister-id"

# Bot Server Configuration (Optional)
OPENCHAT_BOT_PORT="3000"

# Feature Flags (Optional)
OPENCHAT_WELCOME_NEW_MEMBERS="true"

# ElizaOS Configuration (if not already configured)
# Add your other ElizaOS environment variables here
